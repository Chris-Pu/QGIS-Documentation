msgid "Main road"
msgstr "Estrada principal"

#: ../../source/docs/gentle_gis_introduction/vector_attribute_data.rst:376
msgid "Secondary road"
msgstr "Estrada secundária"

#: ../../source/docs/gentle_gis_introduction/vector_attribute_data.rst:378
msgid "Street"
msgstr "Estrada"

