msgstr "Opţiuni:"

#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:444
#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:543
msgid "0 --- Minimum"
msgstr "0 --- Minimum"

#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:445
#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:544
msgid "1 --- Maximum"
msgstr "1 --- Maximum"

#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:446
#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:545
msgid "2 --- Average"
msgstr "2 --- Medie"

#: ../../source/docs/user_manual/processing_algs/taudem/specialized_grid_analysis_tools.rst:448
